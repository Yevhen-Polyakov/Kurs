{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "headers": { "cache-control": "max-age=31536000" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
